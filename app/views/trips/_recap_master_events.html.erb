  <ul class="nav list-group">
    <% @events_master.each do |event| %>
      <% unless event.name == "End" || event.name == "Start" %>
        <li class="list-group-item d-flex justify-content-between align-items-center">
           <p><%= event.name %></p>
           <span class="badge badge-primary badge-pill"><%= event.price %>€</span>
        </li>
      <% end %>
    <% end %>
     <li class="list-group-item d-flex justify-content-between align-items-center">
      <br>
      <br>
      <p>Price of your trip</p>
      <span class="badge badge-primary badge-pill"><%= @total_price %>€</span>
    </li>
  </ul>
